# Multi-Agent Orchestration Framework - Environment Configuration
# Copy this file to .env and fill in your actual values

# ==========================================
# OpenAI Configuration
# ==========================================
# Your OpenAI API key for agent operations
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# ==========================================
# AWS API Gateway Configuration
# ==========================================
# Your AWS API Gateway key for parts and subscription APIs
PARTS_API_KEY=your_aws_api_key_here

# Base URL for the Parts API
PARTS_API_BASE_URL=https://your-api-gateway.amazonaws.com/dev

# ==========================================
# API Endpoints (Optional - uses defaults if not set)
# ==========================================
PARTS_STATUS_ENDPOINT=/parts/status
PARTS_REFUND_ENDPOINT=/parts/refundstatus
PARTS_LOOKUP_ENDPOINT=/parts/lookup
SUBSCRIPTION_LOOKUP_ENDPOINT=/subscription/lookup
SUBSCRIPTION_CANCEL_ENDPOINT=/subscription/cancel
SUBSCRIPTION_EDIT_ENDPOINT=/subscription/edit

# ==========================================
# Optional Configuration
# ==========================================
# Request timeout in seconds
TIMEOUT_SECONDS=30

# Maximum number of retry attempts for failed requests
MAX_RETRIES=3
